
public class Wohnort {

	int plz;
	int hausnummer;
	String Strasse;
	String Ort;
	
	public void setPLZ(int plz)
	{
		this.plz=plz;
	}
	public void setHausnummer(int hausnummer)
	{
		this.hausnummer=hausnummer;
	}
	public void setStrasse(String Strasse)
	{
		this.Strasse=Strasse;
	}
	public void setOrt(String Ort)
	{
		this.Ort=Ort;
	}
	
	
}
